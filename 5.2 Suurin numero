/* Kirjoita ohjelma, joka pyytää käyttäjää syöttämään kaksi kokonaislukua ja kertoo kumpi numeroista oli suurempi, 
tai jos ne olivat yhtäsuuret. */

using System;

class Luvut
{
	static void Main()
	{
		
		int numero1, numero2;

		Console.Write("Syötä ensimmäinen numero: \n");
		numero1 = int.Parse(Console.ReadLine()); //Luetaan ensinmäinen numero

		Console.Write("Syötä toinen numero: \n");
		numero2 = int.Parse(Console.ReadLine()); //Luetaan toinen numero
		
		if (numero1 > numero2)
		{
			Console.WriteLine("Numero 1 on suurempi kuin numero 2.");
		}
		else if (numero1 < numero2)
		{
			Console.WriteLine("Numero 2 on suurempi kuin numero 1.");
		}
		else if (numero1 == numero2)
		{
			Console.WriteLine("Numerot ovat yhtä suuret.");
		}
	}
}
